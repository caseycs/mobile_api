<h3>Structure</h3>

<table class="table table-striped table-condensed">
    <thead>
    <tr>
        <th>Field</th>
        <th>Presence</th>
        <th>Type</th>
        <th>Description</th>
    </tr>
    </thead>
    {{# structure }}
    <tr>
        <td style="padding-left:{{padding_left}}em"><code>{{name}}</code></td>
        <td>{{presence}}</td>
        <td>{{type}}</td>
        <td>
            {{# enum }}
                Possible values:
                <ul>
                {{# values }}
                    <li><code>{{ name }}</code>{{#description}} - {{& description}}{{/description}}</li>
                {{/ values }}
                </ul>
            {{/ enum }}
            {{& description}}
        </td>
    </tr>
    {{/ structure }}
</table>

{{# upload_interface_field}}
<p>Uploaded file required in POST field <code>{{ upload_interface_field }}</code>.</p>
{{/ upload_interface_field}}
